    "koa-onerror": "^4.1.0",
    "koa-router": "^9.0.1",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "log4js": "^6.4.0",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "pm2": "^4.4.0",
    "schema-typed": "^1.5.1",
    "sequelize": "^6.2.4"
  }
