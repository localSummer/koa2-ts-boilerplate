{
  "name": "koa2-ts",
  "version": "1.0.0",
  "description": "koa2 + typescript 开发环境",
  "main": "dist/app.js",
  "scripts": {
    "compile": "tsc --project tsconfig.json -w",
    "dev": "cross-env NODE_ENV=development nodemon bin/www",
    "build": "tsc --project tsconfig.json",
    "prd": "cross-env NODE_ENV=production pm2 start bin/www",
    "clear": "rm -r dist",
    "eslint": "eslint src --ext .ts",
    "gulp-compile": "gulp dev",
    "gulp-build": "gulp build",
    "test": "jest --passWithNoTests --updateSnapshot",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run eslint && npm run test -- --no-cache"
  },
  "keywords": [
    "koa2",
    "typescript"
  ],
  "author": "localsummer",
  "homepage": "https://github.com/localSummer/koa2-ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/localSummer/koa2-ts"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.3",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/koa2-cors": "^2.0.1",
    "@types/koa__multer": "^2.0.1",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.3.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify-es": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "mddir": "^1.1.1",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "sequelize-cli": "^6.1.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@koa/multer": "^3.0.0",
    "debug": "^4.1.1",
